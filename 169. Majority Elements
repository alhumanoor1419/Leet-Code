class Solution:
    def majorityElement(self, nums: List[int]) -> int:
        freq = {}
        n = len(nums)
        threshold = n // 2
        
        
        for num in nums:
            if num in freq:
                freq[num] += 1
            else:
                freq[num] = 1
            
        
            if freq[num] > threshold:
                return num
        
        
        for key, count in freq.items():
            if count > threshold:
                return key
        
        
        return None

